"jim kindley",
2,
"049a111090000412981190109000049012970090109000049012970090109000049012970090100500049012940005900090109000049012970090109000049012910005930090100001049012970090109000049012970090109000049012970090109000049012970090109000049012970090109000049012970090109000029012980f109000049012990f0500",
"easy level conveyor",
"shoot block on to the conveyor to\nstop the tank.",
"reed",
2,"95000b0e9b000b0d0c0e99000b0d90130c0e97000b0d92130c0e95000b0d94130c0e93000b0d96130c0e91000b0d98130c0e000b0d94139000019000130c0e0c0e9413090e050b07130b0d000c0e931393020b0d91000c0e96130b0d93000c0e94130b0d95000c0e92130b0d97000c0e90130b0d99000c0e0b0d9b000c0d9500",
}

difficulties={
  {"kids",10},
  {"easy",12},
  {"medium",11},
  {"hard",2},
  {"deadly",8}
}

modes={
  title_screen=0,
  level_select=1,
  play=2,
  game_over=3,
  win=4,
  pause=5,
  hint=6
}

-- map direction and x,y coords to new coords
directions={
  [1]=function(x,y) return x-1,y end, --left
  [2]=function(x,y) return x+1,y end, --right
  [4]=function(x,y) return x,y-1 end, --up
  [8]=function(x,y) return x,y+1 end  --down
}

opposite_directions={
  [1]=2,
  [2]=1,
  [4]=8,
  [8]=4
}

-- map laser directions and mirrors to new laser direction
mirrors={
  [12]={[2]=4,[8]=1},
  [13]={[8]=2,[1]=4},
  [14]={[1]=8,[4]=2},
  [15]={[2]=8,[4]=1}
}

-- map laser direction and rotating mirror to new laser direction or rotated mirror
rotate_mirrors={
  [21]={[1]={obj=22},[2]={direction=4},[4]={obj=22},[8]={direction=1}},
  [22]={[1]={direction=4},[2]={obj=23},[4]={obj=23},[8]={direction=2}},
  [23]={[1]={direction=8},[2]={obj=24},[4]={direction=2},[8]={obj=24}},
  [24]={[1]={obj=21},[2]={direction=8},[4]={direction=1},[8]={obj=21}}
}

menu={
  {function() level=load_level(selected_level) mode=modes.play end, "play"},
  {function() mode=modes.level_select end, "select level"},
  {function() cam_y=0 mode=modes.instructions end, "instructions"},
}

function _init()
  cartdata("lasertank")
  mode=modes.title_screen
  selected_level=dget(0)
  if (selected_level==0) selected_level=1
  if (selected_level>1) menu[1][2]="continue"
  page=flr(selected_level/20)
  menu_selection=1
end

function _update()
  if mode==modes.title_screen then
    menuitem(1)
    if (btnp(2)) menu_selection=menu_selection==1 and #menu or menu_selection-1
    if (btnp(3)) menu_selection=(menu_selection%#menu)+1
    if (btnp(5)) menu[menu_selection][1]()
  elseif mode==modes.instructions then
    menuitem(1,"title screen",function() mode=modes.title_screen end)
    if (btnp(2) and cam_y<0) cam_y+=5
    if (btnp(3) and cam_y>-160) cam_y-=5
    if (btnp(4)) mode=modes.title_screen
    if (btnp(5)) selected_level=1 level=load_level(1) mode=modes.play
  elseif mode==modes.pause then
    mode=modes.play
  elseif mode==modes.hint then
    if (btnp()!=0) mode=modes.play
  elseif mode==modes.game_over then
    if (btnp(5)) load_level(selected_level) mode=modes.play
  elseif mode==modes.win then
    if btnp(5) then
      selected_level+=1
      if selected_level<=#levels/5 then
        level=load_level(selected_level)
        dset(0,selected_level)
        mode=modes.play
      else
        mode=modes.level_select
      end
    end
  elseif mode==modes.level_select then
    menuitem(1)
    menuitem(2)
    menuitem(3)
    if btnp(2) then
      if (selected_level>1) selected_level-=1
      if (selected_level<(page*20)+1) page-=1
    elseif btnp(3) then
      if (selected_level<#levels/5) selected_level+=1
      if (selected_level>(page*20)+20) page+=1
    end
    if (btnp(5)) level=load_level(selected_level) mode=modes.play dset(0,selected_level)
  elseif mode==modes.play then
    menuitem(1,"show hint",function() mode=modes.hint end)
    menuitem(2,"restart",function() load_level(selected_level) end)
    menuitem(3,"level select",function() mode=modes.level_select end)
    local under=static_actors[tank.y][tank.x].obj
    if (under==1 or under==30 or under>64 or dynamic_actors[tank.y][tank.x] or not last_direction) and not laser then
      -- as long as we're on solid ground and no laser is firing,
      -- we can control the tank
      control=true
    end
    if laser then
      move_laser()
    end
    if control then
      read_keys()
      detect_tank()
    end
    move_ice()
    if (under==3) mode=modes.win
    if (under==4) mode=modes.game_over
    move_conveyor()
  end
end

function _draw()
  if mode==modes.title_screen then
    map()
    --rectfill(18,14,18+88,14+30,1)
    pal(6,1)
    spr(80,22,16,11,3)
    pal()
    cursor(45,54)
    for i=1,#menu do
      cursor(45,45+(9*i))
      color(menu_selection==i and 11 or 6)
      print(menu[i][2])
    end
    spr(27,39,100)
    spr(33,49,100)
    spr(35,70,100)
    spr(11,80,100)
    center("by",102,6)
    center("tobiasvl",108,6)
  elseif mode==modes.instructions then
    local objects={
      {28,"tank","this\nis you"},
      {3,"flag","finish\nlevel"},
      {1,"ground","move &\nshoot\nover"},
      {4,"water","you\ncan't\nswim"},
      {6,"movable\nblock","moved by\nlaser"},
      {30,"bridge","push a\nblock in\nwater"},
      {25,"ice","slide\nacross"},
      {26,"thin\nice","turns\nto\nwater"},
      {19,"tank\nmover","moves\nonly\ntanks"},
      {8,"anti-\ntank","will\nkill you,\nmovable"},
      {20,"crystal\nblock","can\nshoot\nthrough"},
      {5,"solid\nblock","can't be\ndest-\nroyed"},
      {7,"bricks","shoot to\ndestroy"},
      {79,"tunnel","go in,\ncome out\nelse-\nwhere"},
      {13,"mirror","reflects\nlaser,\nmovable"},
      {21,"rotary\nmirror","rotates\nclockwise"}
    }
    cls()
    map(0,0,0,cam_y,16,50)
    rectfill(8,8+cam_y,120,128+cam_y,0)
    color(6)
    local x,y=-22,cam_y+12
    for i=1,#objects do
      x+=36
      spr(objects[i][1],x,y)
      print(objects[i][2],x+9,y,6)
      print(objects[i][3],x,y+12,11)
      if (i%3==0) y+=35 x=-22
    end
    color(7)
    print("         \148",x+44,y)
    print("move:  \139\131\145",x+44,y+6)
    print("shoot:   \151",x+44,y+18)
    print("drive your tank to the",x,y+30)
    print("flag to finish each level.",x,y+36)
    spr(27,24,y+53)
    print("press \151 to play level 1,",x,y+77)
    print(" the lasertank boot camp",x,y+83)
  elseif mode==modes.play then
    cls()
    for t in all({static_actors,dynamic_actors}) do
      for y=0,15 do
        for x=0,15 do
          local v=t[y][x]
          if (v) mset(v.x,v.y,v.obj)
        end
      end
      map()
    end
    if (laser) mset(laser.x,laser.y,laser.obj)
    mset(tank.x,tank.y,tank.obj)
    map()
  elseif mode==modes.game_over then
    local msg="you are dead !!!"
    rect(64-(#msg*2)-2,62,64+(#msg*2),70,8)
    rectfill(64-(#msg*2)-1,63,64+(#msg*2)-1,69,0)
    center(msg,64,8)
  elseif mode==modes.win then
    local msg="you win !!!"
    rect(64-(#msg*2)-2,62,64+(#msg*2),70,11)
    rectfill(64-(#msg*2)-1,63,64+(#msg*2)-1,69,0)
    center(msg,64,11)
  elseif mode==modes.pause then
    rectfill(0,0,127,24,0)
    center(level.title)
    center("by",6,5)
    center(level.author,12)
    local difficulty=difficulties[level.difficulty]
    if difficulty then
      center(level.number.." - "..difficulty[1],18,difficulty[2])
    else
      center(tostr(level.number),18)
    end
  elseif mode==modes.hint then
    rectfill(0,25,127,127,0)
    local y=32
    center("hint:",y,5)
    for i=1,256,32 do
      y+=6
      print(sub(level.hint,i,i+32),0,y,7)
    end
  elseif mode==modes.level_select then
    cls()
    for i=(page*20)+1,(page*20)+20 do
      if i<=#levels/5 then
        if selected_level==i then
          local y=peek(0x5f27)
          rectfill(peek(0x5f26),y,127,y+5,4)
        end
        if (difficulties[levels[((i-1)*5)+5]]) color(difficulties[levels[((i-1)*5)+5]][2]) else color(7)
        print(i.." "..levels[((i-1)*5)+2])
      end
    end
    if (page>0) print("\148",120,0)
    if ((page*20)+20<#levels/5) print("\131",120,114)
  end
end

function center(str,y,c)
  c=c or 7
  print(str,64-(#str*2),y,c)
end

function read_keys()
  local button=btnp()
  if button==0x40 then
    mode=modes.pause
  end
  if control and not laser and button==0x20 then
    laser={x=tank.x,y=tank.y,direction=tank.direction}
    move_laser()
  elseif button==0x10 then
    -- o
  elseif control and not laser and button!=0 then
    if tank.direction!=button then
      turn_tank(button)
    else
      move_tank(button)
    end
  end
end

function turn_tank(button)
  local tank_sprites={
    [1]=29,
    [2]=27,
    [4]=2,
    [8]=28
  }
  local spr=tank_sprites[button]
  if (spr) tank.direction,tank.obj=button,spr
  return
end

function move_tank(button)
  if (not button) button=last_direction
  if (not button) return
  last_direction=button
  local new_x,new_y=directions[button](tank.x,tank.y)
  local under=mget(new_x,new_y)
  if fget(under,1) then
    -- solid object
    control=true
    last_direction=nil
  elseif new_x<0 or new_x>15 or new_y<0 or new_y>15 then
    -- out of bounds
    control=true
  else
    if under>64 then
      -- tunnel
      if (not tunnels[under]) mode=modes.game_over return
      for t in all(tunnels[under]) do
        if not ((t.x==new_x and t.y==new_y) or dynamic_actors[t.y][t.x]) then
          tank.x,tank.y=t.x,t.y
          return
        end
      end
    end
    tank.x,tank.y=new_x,new_y
    if control then
      if under==16 or under==17 or under==18 or under==19 then
        control=false
      end
      if (under==25) control=false
      if (under==26) control=false
    end
  end
end

function move_laser()
  if (not laser) return
  local new_x,new_y=directions[laser.direction](laser.x,laser.y)
  if (new_x==tank.x and new_y==tank.y) mode=modes.game_over
  local obj=mget(new_x,new_y)
  if mirrors[obj] and mirrors[obj][laser.direction] then
    laser.x,laser.y=new_x,new_y
    laser.obj=(laser.direction==4 or laser.direction==8) and 32 or 33
    laser.direction=mirrors[obj][laser.direction]
  elseif fget(obj,0) then
    -- movable object
    move_object(new_x,new_y,laser.direction)
    laser=nil
  elseif rotate_mirrors[obj] then
    -- rotatable mirror
    laser.x,laser.y=new_x,new_y
    if rotate_mirrors[obj][laser.direction] and rotate_mirrors[obj][laser.direction].direction then
      laser.obj=(laser.direction==4 or laser.direction==8) and 32 or 33
      laser.direction=rotate_mirrors[obj][laser.direction].direction
    elseif rotate_mirrors[obj][laser.direction].obj then
      static_actors[new_y][new_x].obj=rotate_mirrors[obj][laser.direction].obj
      laser=nil
    end
  elseif fget(obj,1) and obj!=20 then
    -- solid object
    if (obj==7) static_actors[new_y][new_x].obj=1
    laser=nil
  elseif new_x<0 or new_x>15 or new_y<0 or new_y>15 then
    -- out of bounds
    laser=nil
  else
    laser.x,laser.y,laser.obj=new_x,new_y,(laser.direction==4 or laser.direction==8) and 32 or 33
    new_x,new_y=directions[laser.direction](laser.x,laser.y)
  end
end

function move_object(x,y,direction)
  local new_x,new_y=directions[direction](x,y)
  local obj=mget(x,y)
  if (obj==8 and direction==8) or (obj==9 and direction==1) or (obj==10 and direction==4) or (obj==11 and direction==2) then
    -- destroy antitank
    dynamic_actors[y][x].obj+=28
  elseif new_x<0 or new_x>15 or new_y<0 or new_y>15 or fget(mget(new_x,new_y),1) then
    -- move against out of bounds
    -- move against solid object
  elseif mget(new_x,new_y)==4 then
    -- move into water
    if (obj==6) static_actors[new_y][new_x]={x=new_x,y=new_y,obj=30}
    dynamic_actors[y][x]=nil
    control=false -- todo: hack?
  else
    if static_actors[new_y][new_x].obj>64 then
      -- tunnel
      for t in all(tunnels[static_actors[new_y][new_x].obj]) do
        if not ((t.x==new_x and t.y==new_y) or (dynamic_actors[t.y][t.x] and not (t.x==x and t.y==y))) then
          dynamic_actors[t.y][t.x]={x=t.x,y=t.y,obj=dynamic_actors[y][x].obj}
          if (not (t.x==x and t.y==y)) dynamic_actors[y][x]=nil
          control=false
          return
        end
      end
    end
    -- move object
    dynamic_actors[new_y][new_x]=dynamic_actors[y][x]
    dynamic_actors[new_y][new_x].x=new_x
    dynamic_actors[new_y][new_x].y=new_y
    dynamic_actors[y][x]=nil
    control=false -- todo: hack?
    if static_actors[new_y][new_x].obj==25 then
      dynamic_actors[new_y][new_x].direction=direction
      add(objects_on_ice,dynamic_actors[new_y][new_x])
    elseif static_actors[new_y][new_x].obj==26 then
      static_actors[new_y][new_x].obj=4
      dynamic_actors[new_y][new_x].direction=direction
      add(objects_on_ice,dynamic_actors[new_y][new_x])
    end
  end
end

function detect_tank()
  if (dynamic_actors[tank.y][tank.x]) return
  if (laser) return
  for dir in all({2,1,8,4}) do
    local move,x,y=directions[dir],tank.x,tank.y
    repeat
      x,y=move(x,y)
    until fget(mget(x,y),1) or x<0 or x>15 or y<0 or y>15
    if (mget(x,y)==8 and dir==8) or (mget(x,y)==9 and dir==1) or (mget(x,y)==10 and dir==4) or (mget(x,y)==11 and dir==2) then
      laser={x=x,y=y,direction=opposite_directions[dir]}
      laser.obj=laser.direction==1 or laser.direction==2 and 35 or 34
      break
    end
  end
  move_laser()
end

function move_ice()
  for obj in all(objects_on_ice) do
    local x,y=obj.x,obj.y
    local new_x,new_y=directions[obj.direction](x,y)
    if not (tank.x==new_x and tank.y==new_y) then
      move_object(x,y,obj.direction)
    end
    if static_actors[y][x].obj==4 then
      -- move into water
      if (obj.obj==6) static_actors[y][x].obj=30
      dynamic_actors[y][x]=nil
      control=false -- todo: hack?
    end
    del(objects_on_ice,obj)
  end
end

function move_conveyor()
  local under=static_actors[tank.y][tank.x].obj
  if (not laser) detect_tank()
  if not dynamic_actors[tank.y][tank.x] then
    if (under==16) move_tank(4)
    if (under==17) move_tank(2)
    if (under==18) move_tank(8)
    if (under==19) move_tank(1)
    if not control then
      if (under==25) move_tank()
      if (under==26) static_actors[tank.y][tank.x]={x=tank.x,y=tank.y,obj=4} move_tank()
    end
  end
  detect_tank()
end

function load_level(lvl_number)
  local lvl=levels[((lvl_number-1)*5)+1]
  static_actors={}
  for y=0,15 do
    static_actors[y]={}
    for x=0,15 do
      static_actors[y][x]={x=x,y=y,obj=1} --ground
    end
  end
  dynamic_actors={}
  for y=0,15 do
    dynamic_actors[y]={}
    for x=0,15 do
      dynamic_actors[y][x]=nil
    end
  end
  tunnels={}
  for t=65,79,2 do
    tunnels[t]={}
  end
  local s=1
  local actual_s=1
  local byte=tonum("0x"..sub(lvl,s,s+1))
  while byte do -- 0xa0
    local run=1
    if band(byte,0xf0)==0x90 then
      run=band(byte,0x0f)+2
      s+=2
      obj=tonum("0x"..sub(lvl,s,s+1))+1
    else
      obj=byte+1
    end
    for i=1,run do
      y=((actual_s-1)%32)/2
      x=flr((actual_s-1)/32)

      if obj==2 then
        print("found")
        tank={x=x,y=y,direction=4,obj=2}
      elseif fget(obj,0) then
        dynamic_actors[y][x]={x=x,y=y,obj=obj}
        static_actors[y][x]={x=x,y=y,obj=1}
      else
        static_actors[y][x]={x=x,y=y,obj=obj}
      end
      actual_s+=2
    end
    for y=0,15 do
      for x=0,15 do
        obj=static_actors[y][x].obj
        if obj>64 then
          add(tunnels[obj],{x=x,y=y})
        end
      end
    end
    s+=2
    byte=tonum("0x"..sub(lvl,s,s+1))
  end
  laser=nil
  control=true
  objects_on_ice={}
  return {number=lvl_number,title=levels[((lvl_number-1)*5)+2],hint=levels[((lvl_number-1)*5)+3],author=levels[((lvl_number-1)*5)+4],difficulty=levels[((lvl_number-1)*5)+5]}
end
